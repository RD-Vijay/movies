pipeline {
    agent any
    options {
        skipStagesAfterUnstable()
    }
    stages {
        stage('Build') {
            steps {
               echo "Jenkins started"
            }
        }
        stage('Test'){
            steps {
                echo "Second Stage"
            }
        }
        stage('Deploy') {
            steps {
                echo 'Final stage'
            }
        }
    }
}
